        <script>
            if (window.location.pathname.includes("novos/haval-h6-hev")) {
                var text = "Com teto solar (opcional): R$ 224.000";
            } else if (window.location.pathname.includes("novos/haval-h6-phev")) {
                var text = "Com teto solar (opcional): R$ 279.000";
            }

            if (text) {
                const ctaReservation = document.getElementById("cta-reservation");
                const newElement = document.createElement("h6");
