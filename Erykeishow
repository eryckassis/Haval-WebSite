        <script>
            const urlAtual = window.location.href;
            const utmMatches = urlAtual.match(/[?&]utm_([^&]+)/gi);
            const utmValues = utmMatches ? utmMatches.join('') : null;

            if (utmValues) {
                const outraUrl = 'https://gwmmotors.mercadolivre.com.br';
                const novaUrl = `${outraUrl}${utmValues}`;
                const linksAntigos = document.querySelectorAll(`a[href*="https://gwmmotors.mercadolivre.com.br/?utm_source=gwmsite"]`);

                linksAntigos.forEach(link=>{
                    link.href = link.href.replace('https://gwmmotors.mercadolivre.com.br/?utm_source=gwmsite', novaUrl);
                }
                );
            }
        </script>
