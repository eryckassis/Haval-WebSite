            $(document).ready(function() {
                function e(e) {
                    for (var s = ["youtube", "vimeo", "mp4"], t = 0; t < s.length; t++)
                        if (-1 !== e.indexOf(s[t]))
                            return s[t]
                }
                function s(s) {
                    return s.map(function(s) {
                        var t = e(s);
                        switch (t) {
                        case "mp4":
                            return {
                                type: t,
                                options: {
                                    source: {
                                        type: "video",
                                        sources: [{
                                            src: s,
                                            type: "video/mp4"
                                        }]
                                    }
                                }
                            };
                        case "youtube":
                        case "vimeo":
                            return {
                                type: t,
                                embedId: s
                            }
                        }
                    })
                }
                var t = document.querySelector(".gallery-video__gallery");
                if (!t)
                    return;
                const o = ()=>["iPad Simulator", "iPhone Simulator", "iPod Simulator", "iPad", "iPhone", "iPod"].includes(navigator.platform) || navigator.userAgent.includes("Mac") && "ontouchend"in document;
